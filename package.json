{
  "name": "express-typescript-typeorm",
  "version": "0.0.1",
  "main": "src/server.ts",
  "license": "MIT",
  "scripts": {
    "start": "tsc && nodemon --exec ts-node src/server.ts",
    "tsc": "tsc",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migrate:create": "tsc && typeorm migration:create -n",
    "migrate:generate": "tsc && typeorm migration:generate -n",
    "migrate:run": "tsc && typeorm migration:run",
    "migrate:revert": "tsc && typeorm migration:revert",
    "schema:sync": "tsc && typeorm schema:sync"
  },
  "dependencies": {
    "@types/express": "^4.17.11",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "mysql2": "^2.2.5",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.30"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/node": "^14.14.45",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  }
}
