{
  "name": "express-typescript-typeorm",
  "version": "0.0.1",
  "main": "src/server.ts",
  "license": "MIT",
  "scripts": {
    "start": "node dist/app.js",
    "start:production": "tsc && node dist/app.js",
    "start:dev": "nodemon --exec ts-node src/app.ts",
    "tsc": "tsc",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix && prettier --write src/**/*.ts",
    "build": "yarn tsc && yarn lint",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migrate:create": "tsc && typeorm migration:create -n",
    "migrate:generate": "tsc && typeorm migration:generate -n",
    "migrate:run": "tsc && typeorm migration:run",
    "migrate:revert": "tsc && typeorm migration:revert",
    "schema:sync": "tsc && typeorm schema:sync",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format"
    }
  },
  "dependencies": {
    "@types/express": "^4.17.11",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "path": "^0.12.7",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.30",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/jwk-to-pem": "^2.0.0",
    "@types/luxon": "^1.26.5",
    "@types/node": "^15.6.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "^5.5.3",
    "graphql": "^15.5.0",
    "husky": "^6.0.0",
    "jest": "^27.0.3",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  }
}
